func _on_player_entered(body: Node2D) -> void:
	if body.is_in_group(\"players\"):
		var lines: Array[String] = [\"Come up!\", \"Jump! (space)\"]
		body_entered.disconnect(_on_player_entered)
		old_man.say(lines, text_box_position.global_position)
"

